<section>
    <h1>New Project</h1>
    <%= form_with model: @project, local: true do |form| %>
        <div>
            <%= form.label :name %><br>
            <%= form.text_field :name %>
        </div>
        <div>
            <%= form.submit "Create Project" %>
        </div>
    <%end%>
</section>