<%= render Templates::ListTemplateComponent.new(
      table: {
        header: "Projects",
        items: @projects,
        columns: [
          {
            label: 'ID',
            accessor: :id
          },
          {
            label: 'Name',
            accessor: :name
          }
        ],
        actions: [
          {
            label: "View",
            path_proc: proc { |project| project_path(project) }
          }
        ]
      },           
)%>