<section>
    <h1>Edit Project</h1>

    <%= form_with model: @project, local: true do |form| %>
        <div>
            <%= form.label :name %><br>
            <%= form.text_field :name %>
        </div>

        <div>
            <%= form.submit "Update Project"  %>
        </div>
    <%end%>
    <%= link_to "Back to Projects", projects_path %>
</section>